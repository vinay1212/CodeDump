#include<iostream>
#include<bits/stdc++.h>
using namespace std;

int main() {
	int n;
	cin>>n;
    if(n < 1){
        return 0;
    }
    int step = 2;
    cout<<"1"<<endl;
    while(step <= n){
        cout<<step;
        cout<<"\t";
        for(int i = 1; i < step-1; i++){
            cout<<"0";
            cout<<"\t";
        }
        cout<<step;
        step++;
        cout<<endl;
    }
	return 0;
}
